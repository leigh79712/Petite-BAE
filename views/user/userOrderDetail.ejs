<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/style.css" />

    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto+Mono:wght@100;400;700&display=swap"
      rel="stylesheet"
    />
    <link rel="shortcut icon" type="image/png" href="/pic/logo.png" />
    <script defer src="/javascript/script.js"></script>
    <link rel="stylesheet" href="/css/order.css" />
    <script>
      const orderDetail = <%- JSON.stringify(order) %>
    </script>
    <link rel="stylesheet" href="/css/shoppingCart.css" />

    <script defer src="/javascript/order.js"></script>
    <title>Petite BAE</title>
  </head>
  <body>
    <%- partial("./partial/navbar") %>
    <div class="container mt-5">
      <div class="row">
        <%- partial("./partial/userpagesidebar") %>

        <div class="col">
          <div class="row">
            <div class="col-3"><h5>Order</h5></div>
            <div class="col">
              <h6>Username</h6>
            </div>
            <div class="col">
              <h6>First Name</h6>
            </div>
            <div class="col">
              <h6>Last Name</h6>
            </div>
            <div class="col">
              <h6>E-mail</h6>
            </div>
          </div>

          <div class="mb-3">
            <div class="row">
              <div class="col"><h6><%= order.id%></h6></div>
              <div class="col">
                <h6><%= order.user.username %></h6>
              </div>
              <div class="col">
                <h6><%= order.user.firstname %></h6>
              </div>
              <div class="col">
                <h6><%= order.user.lastname %></h6>
              </div>
              <div class="col">
                <h6><%= order.user.email %></h6>
              </div>
            </div>
          </div>
          <div class="row">
            <div id="appendHtml" class="center offset-1"></div>
          </div>
        </div>
      </div>
    </div>
    <%- partial("./partial/footer") %>
  </body>
</html>
